# Actions
import move:"com.goldev.skipwave.ai.MoveTask"
import attack:"com.goldev.skipwave.ai.AttackTask"

# Conditions
import canAttack?:"com.goldev.skipwave.ai.CanAttack"

subtree name:"attack"
    sequence
        (canAttack?) attack
        wait seconds:"uniform,1"

subtree name:"move"
  sequence
    move range:0

root
  selector
    $attack
    $move


